= form_for @job_posting_skill_set do |f|
  - if @job_posting_skill_set.errors.any?
    #error_explanation
      %h2= "#{pluralize(@job_posting_skill_set.errors.count, "error")} prohibited this job_posting_skill_set from being saved:"
      %ul
        - @job_posting_skill_set.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :job_posting_id
    = f.number_field :job_posting_id
  .field
    = f.label :name
    = f.text_field :name
  .actions
    = f.submit 'Save'
