		$("#search_user_name").show();
		$(".searchresult").show();
		$(".searchresult").html("<%= j render('skill_sets/search_user') %>")
	<%else%>
		$("#search_user_name").hide();
		$(".searchresult").html("<%= j render('invites/invites_user') %>")
	<%end%>
<%end%>
